<!DOCTYPE html>
<html lang="es">
  <head>
    <meta
      charset="UTF-8"
      name="viewport"
      content="width=device-width, initial-scale=1.0"
    />

    <link
      rel="icon"
      type="image/x-icon"
      href="/static/icon/favicon.ico"
    />

    <link
      rel="stylesheet"
      type="text/css"
      href="/static/css/login.css"
    />

    <title><%= title %></title>
  </head>

  <body>
    <div class="wrapper">
      <header>
        <h1>Bienvenidos</h1>
        <p>Restaurantes</p>
      </header>

      <main>
        <% data.forEach((item) => { %>
          <div>
            <p><%= item.shopname %>: <%= item.password %></p>
          </div>
        <% }); %>
      </main>
    </div>
  </body>
</html>
